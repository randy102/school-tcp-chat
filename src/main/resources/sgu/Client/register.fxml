<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sgu.Client.RegisterController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="nameInput" onAction="#onRegisterClick" promptText="Your name..." />
    <Button onAction="#onRegisterClick" prefHeight="20.0" prefWidth="102.0" style="-fx-background-color: #336ea1;" text="Join" textFill="WHITE" />
</VBox>
